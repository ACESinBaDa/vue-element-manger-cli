.el-table th
  background-color #EFF2F6
  font-size 12px
  color #525F75
  .cell
    font-weight bold
.el-table td
  border-bottom none
.border.el-table td
  box-shadow 0 -1px 0 #E0E6ED
.el-dialog__header
  box-shadow 0 1px 0 #E8E8ED
.tooltipItem
  max-width 400px
.tooltipOverflow
  display inline-block
  width 128px
  overflow hidden
  text-overflow ellipsis
  white-space nowrap
/* 加载中页面 */
.el-loading-mask
  background-color rgba(0, 0, 0, 0.5)